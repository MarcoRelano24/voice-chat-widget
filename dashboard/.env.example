# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Widget CDN URL (your deployed widget location)
NEXT_PUBLIC_WIDGET_CDN_URL=http://localhost:5173

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Vapi Private API Key (get this from https://vapi.ai dashboard)
# IMPORTANT: This should be your PRIVATE key, not the public key!
VAPI_PRIVATE_KEY=your_vapi_private_key_here

# Vapi Public API Key (get this from https://vapi.ai dashboard)
# This will be used by all widgets if they don't have their own key configured
# Setting this allows you to create widgets without entering the public key each time
NEXT_PUBLIC_VAPI_PUBLIC_KEY=your_vapi_public_key_here
